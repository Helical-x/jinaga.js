    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "express": "^4.18.1",
    "node-forge": "^1.3.1",
    "passport": "^0.5.2",
    "pg": "^8.7.1",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1"
  }
}

{
  "name": "jinaga",
  "version": "3.0.8",
  "description": "Application-agnostic back end for web applications",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "webpack",
    "test": "mocha --opts mocha.opts",
    "test:watch": "mocha --opts mocha.opts --watch",
    "dev": "nodemon -e ts -w ./src -x npm run test:watch",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "integration": "npm run build && cd ./integration-test && ./run.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michaellperry/jinaga.git"
  },
  "keywords": [
    "messaging"
  ],
  "author": "Michael L Perry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaellperry/jinaga/issues"
  },
  "homepage": "http://jinaga.com",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/express": "^4.17.13",
    "@types/mocha": "^7.0.2",
    "@types/node-forge": "^0.10.10",
    "@types/passport": "^1.0.7",
    "@types/pg": "^8.6.1",
    "acorn": "^7.4.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "del": "^5.1.0",
    "mocha": "^7.2.0",
